{
    "rewrites": [
        {
            "source": "/api/webhook",
            "destination": "/api/webhook"
        },
        {
            "source": "/api/process-payment",
            "destination": "/api/process-payment"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/main"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}